const readline = require('readline');

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (Data) => {
    var inputData = Data.split(" ");
    var number =parseInt(inputData[0]);
     var fact = 1;
     for(var i=1;i<=number;i++){
         fact = fact*i;
     }
     console.log(fact);
     
     });
